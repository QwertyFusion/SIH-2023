<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="accountPageVM" type="com.sapplication.app.modules.accountpage.data.viewmodel.AccountPageVM"/>
    </data>
    <LinearLayout android:gravity="start"
        android:id="@+id/linearColumntopleft"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylegray_200">
        <LinearLayout android:gravity="start"
            android:id="@+id/linearAccountPage"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylegray_200">
            <LinearLayout android:id="@+id/linearRowtopleft"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:orientation="horizontal" style="@style/groupStylecyan_404">
                <FrameLayout android:id="@+id/frameStacktopleft"
                    android:layout_height="@dimen/_168pxv"
                    android:layout_marginBottom="@dimen/_15pxv" android:layout_width="@dimen/_264pxh">
                    <ImageView android:id="@+id/imageTopLeft"
                        android:layout_gravity="top|start"
                        android:layout_height="@dimen/_155pxh"
                        android:layout_marginBottom="@dimen/_10pxv"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_width="@dimen/_162pxh"
                        android:scaleType="fitXY"
                        imageRes="@{@drawable/img_topleft_cyan_50}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_topleft_cyan_50"/>
                    <TextView android:ellipsize="end"
                        android:gravity="top|start"
                        android:id="@+id/txtHelloAdamSte"
                        android:layout_gravity="bottom|end"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_10pxh"
                        android:layout_marginTop="@dimen/_10pxv"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text="@{accountPageVM.accountPageModel.txtHelloAdamSte}"
                        style="@style/txtOpenSansromanbold18_1" tools:text="@string/msg_hello_adam_ste"/>
                    <LinearLayout android:id="@+id/linearUserIcon"
                        android:layout_gravity="end"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_32pxv"
                        android:layout_marginEnd="@dimen/_34pxh"
                        android:layout_marginStart="@dimen/_34pxh"
                        android:layout_marginTop="@dimen/_32pxv"
                        android:layout_width="wrap_content"
                        android:orientation="vertical" style="@style/groupStylebluegray_100cornerRadius">
                        <ImageView android:id="@+id/imageLayer251"
                            android:layout_gravity="start"
                            android:layout_height="@dimen/_100pxh"
                            android:layout_width="@dimen/_100pxh"
                            android:scaleType="centerCrop"
                            circular="@{true}"
                            imageRes="@{@drawable/img_layer251}"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_layer251"/>
                    </LinearLayout>
                </FrameLayout>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearColumnclose"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_109pxv"
                    android:layout_marginEnd="@dimen/_30pxh"
                    android:layout_marginStart="@dimen/_41pxh"
                    android:layout_marginTop="@dimen/_14pxv"
                    android:layout_weight="0.09"
                    android:layout_width="0dp" android:orientation="vertical">
                    <ImageView android:id="@+id/imageClose"
                        android:layout_height="@dimen/_25pxh"
                        android:layout_width="match_parent"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_close"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_close"/>
                    <ImageView android:gravity="center_horizontal"
                        android:id="@+id/imagePrinter"
                        android:layout_height="@dimen/_21pxh"
                        android:layout_marginEnd="@dimen/_1pxh"
                        android:layout_marginStart="@dimen/_2pxh"
                        android:layout_marginTop="@dimen/_14pxv"
                        android:layout_width="match_parent"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_printer"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_printer"/>
                </LinearLayout>
            </LinearLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearAccountPageCo"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylegray_200">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtEmergency"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_25pxh"
                    android:layout_marginStart="@dimen/_25pxh"
                    android:layout_marginTop="@dimen/_22pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{accountPageVM.accountPageModel.txtEmergency}"
                    style="@style/txtOpenSansromanbold18" tools:text="@string/lbl_emergency"/>
                <TextView android:ellipsize="end"
                    android:gravity="center_horizontal"
                    android:id="@+id/txtAmbulanceButto"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_25pxh"
                    android:layout_marginStart="@dimen/_25pxh"
                    android:layout_marginTop="@dimen/_5pxv"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_19pxv"
                    android:paddingStart="@dimen/_20pxh"
                    android:paddingTop="@dimen/_19pxv"
                    android:singleLine="true"
                    android:text="@{accountPageVM.accountPageModel.txtAmbulanceButto}"
                    style="@style/txtSolidRoundedTopCornerOutline" tools:text="@string/lbl_ambulance"/>
                <TextView android:ellipsize="end"
                    android:gravity="center_horizontal"
                    android:id="@+id/txtHospitalButton"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_25pxh"
                    android:layout_marginStart="@dimen/_25pxh"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_17pxv"
                    android:paddingStart="@dimen/_20pxh"
                    android:paddingTop="@dimen/_21pxv"
                    android:singleLine="true"
                    android:text="@{accountPageVM.accountPageModel.txtHospitalButton}"
                    style="@style/txtSolidOutline" tools:text="@string/lbl_hospital"/>
                <TextView android:ellipsize="end"
                    android:gravity="center_horizontal"
                    android:id="@+id/txtMedicineStore"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_25pxh"
                    android:layout_marginStart="@dimen/_25pxh"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_19pxv"
                    android:paddingStart="@dimen/_20pxh"
                    android:paddingTop="@dimen/_20pxv"
                    android:singleLine="true"
                    android:text="@{accountPageVM.accountPageModel.txtMedicineStore}"
                    style="@style/txtSolidRoundedBottomCornerOutline" tools:text="@string/lbl_medicine_store"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtAccountSetting"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_25pxh"
                    android:layout_marginStart="@dimen/_25pxh"
                    android:layout_marginTop="@dimen/_24pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{accountPageVM.accountPageModel.txtAccountSetting}"
                    style="@style/txtOpenSansromanbold18" tools:text="@string/msg_account_setting"/>
                <TextView android:ellipsize="end"
                    android:gravity="center_horizontal"
                    android:id="@+id/txtDataButton"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_25pxh"
                    android:layout_marginStart="@dimen/_25pxh"
                    android:layout_marginTop="@dimen/_6pxv"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_17pxv"
                    android:paddingStart="@dimen/_20pxh"
                    android:paddingTop="@dimen/_21pxv"
                    android:singleLine="true"
                    android:text="@{accountPageVM.accountPageModel.txtDataButton}"
                    style="@style/txtSolidRoundedTopCornerOutline_1" tools:text="@string/msg_login_and_secur"/>
                <TextView android:ellipsize="end"
                    android:gravity="center_horizontal"
                    android:id="@+id/txtFamilyButton"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_25pxh"
                    android:layout_marginStart="@dimen/_25pxh"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_17pxv"
                    android:paddingStart="@dimen/_20pxh"
                    android:paddingTop="@dimen/_21pxv"
                    android:singleLine="true"
                    android:text="@{accountPageVM.accountPageModel.txtFamilyButton}"
                    style="@style/txtSolidOutline_1" tools:text="@string/lbl_family_connect"/>
                <TextView android:ellipsize="end"
                    android:gravity="center_horizontal"
                    android:id="@+id/txtButtonThree"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_25pxh"
                    android:layout_marginStart="@dimen/_25pxh"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_16pxv"
                    android:paddingStart="@dimen/_20pxh"
                    android:paddingTop="@dimen/_22pxv"
                    android:singleLine="true"
                    android:text="@{accountPageVM.accountPageModel.txtButtonThree}"
                    style="@style/txtSolidOutline_1" tools:text="@string/lbl_manage_devices"/>
                <TextView android:ellipsize="end"
                    android:gravity="center_horizontal"
                    android:id="@+id/txtButtonFour"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_25pxh"
                    android:layout_marginStart="@dimen/_25pxh"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_17pxv"
                    android:paddingStart="@dimen/_20pxh"
                    android:paddingTop="@dimen/_21pxv"
                    android:singleLine="true"
                    android:text="@{accountPageVM.accountPageModel.txtButtonFour}"
                    style="@style/txtSolidOutline_1" tools:text="@string/msg_manage_your_pro"/>
                <View
                    android:background="@drawable/rectangle_bg_gray_200_border_black_900_26"
                    android:id="@+id/viewRectangleOne"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_12pxv"
                    android:layout_marginEnd="@dimen/_25pxh"
                    android:layout_marginStart="@dimen/_25pxh" android:layout_width="@dimen/_310pxh"/>
            </LinearLayout>
        </LinearLayout>
        <FrameLayout android:id="@+id/frameBottombar"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" style="@style/groupStylecyan_405">
            <LinearLayout android:id="@+id/linearRowglobe"
                android:layout_height="match_parent"
                android:layout_marginBottom="@dimen/_10pxv"
                android:layout_marginTop="@dimen/_9pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <FrameLayout android:id="@+id/frameWebButton"
                    android:layout_height="@dimen/_35pxv"
                    android:layout_marginTop="@dimen/_5pxv"
                    android:layout_width="@dimen/_35pxh" style="@style/groupStylecyan_404cornerRadius">
                    <ImageView android:id="@+id/imageGlobe"
                        android:layout_gravity="center"
                        android:layout_height="@dimen/_23pxh"
                        android:layout_marginHorizontal="@dimen/_5pxh"
                        android:layout_marginVertical="@dimen/_5pxv"
                        android:layout_width="@dimen/_23pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_globe"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_globe"/>
                </FrameLayout>
                <FrameLayout android:id="@+id/frameDoctorButton"
                    android:layout_height="@dimen/_35pxv"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginTop="@dimen/_4pxv"
                    android:layout_width="@dimen/_35pxh" style="@style/groupStylecyan_404cornerRadius">
                    <ImageView android:id="@+id/imageThumbsup"
                        android:layout_gravity="center"
                        android:layout_height="@dimen/_21pxh"
                        android:layout_marginBottom="@dimen/_7pxv"
                        android:layout_marginTop="@dimen/_7pxv"
                        android:layout_width="@dimen/_22pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_thumbsup"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_thumbsup"/>
                </FrameLayout>
                <ImageView android:id="@+id/imageLock"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_40pxh"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginStart="@dimen/_33pxh"
                    android:layout_width="@dimen/_40pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_lock"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_lock"/>
                <FrameLayout android:id="@+id/frameHospitalButton"
                    android:layout_height="@dimen/_35pxv"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginTop="@dimen/_4pxv"
                    android:layout_width="@dimen/_35pxh" style="@style/groupStylecyan_404cornerRadius">
                    <ImageView android:id="@+id/imageHome"
                        android:layout_gravity="center"
                        android:layout_height="@dimen/_22pxh"
                        android:layout_marginBottom="@dimen/_10pxv"
                        android:layout_marginTop="@dimen/_5pxv"
                        android:layout_width="@dimen/_25pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_home"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_home"/>
                </FrameLayout>
                <FrameLayout android:id="@+id/frameFAQButton"
                    android:layout_height="@dimen/_33pxv"
                    android:layout_marginBottom="@dimen/_2pxv"
                    android:layout_marginTop="@dimen/_5pxv"
                    android:layout_width="@dimen/_33pxh" style="@style/groupStylecyan_1">
                    <ImageView android:id="@+id/imageUser"
                        android:layout_gravity="center"
                        android:layout_height="@dimen/_19pxh"
                        android:layout_marginBottom="@dimen/_6pxv"
                        android:layout_marginTop="@dimen/_7pxv"
                        android:layout_width="@dimen/_25pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_user"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_user"/>
                </FrameLayout>
            </LinearLayout>
        </FrameLayout>
    </LinearLayout>
</layout>
